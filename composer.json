{
    "name": "omnifraud/kount",
    "description": "Kount driver for the Omnifraud PHP fraud prevention library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "LXRandCo",
            "email": "hugo.v@lxrco.com"
        }
    ],
    "require": {
        "omnifraud/common": "^0.1.1",
        "kount/kount-ris-php-sdk": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Omnifraud\\Kount\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ],
        "lint": [
            "phpcs --standard=PSR2 src"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
